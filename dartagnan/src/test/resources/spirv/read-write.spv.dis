               OpCapability Shader
        %ext = OpExtInstImport "GLSL.std.450"
               OpMemoryModel Logical GLSL450
               OpEntryPoint GLCompute %main "main"
               OpSource GLSL 450
       %void = OpTypeVoid
       %func = OpTypeFunction %void
       %uint = OpTypeInt 64 0
   %ptr_uint = OpTypePointer Uniform %uint
         %c1 = OpConstant %uint 1
         %c2 = OpConstant %uint 2
         %v1 = OpVariable %ptr_uint Uniform %c1
         %v2 = OpVariable %ptr_uint Uniform %c2
       %main = OpFunction %void None %func
      %label = OpLabel
         %l1 = OpLoad %uint %v1
         %l2 = OpLoad %uint %v2
               OpStore %v1 %l2
               OpStore %v2 %l1
               OpReturn
               OpFunctionEnd
