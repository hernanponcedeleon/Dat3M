PTX fence
"Two thread example with a constant proxy fence"
{
rd1 = 0;
rd2 @ generic virtually aliases rd1;
P0:%r0=0;
}
 P0@cta 0,gpu 0                   |P1@cta 1,gpu 0                   ;
 st.weak rd1, 42                  |ld.acquire.cta %r5, rd4          ;
 fence.proxy.constant             |ld.weak %r3, rd2                 ;
 st.release.cta rd4, 1            |                                 ;
exists
(P1:%r5 == 1 /\ P1:%r3 != 42)