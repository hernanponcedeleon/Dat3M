BPF ARMv8DOB5-PPC

(* A write W3 is ordered after a Write W2 if a read R1 that is po before
 * W2, reads a value that is used to compute the address of W2.
 *
 * R1 ->addr W2 ->po W3
 *
 * Result: Never
 *)

{
0:r2=x; 0:r4=y; 0:r6=z;
1:r2=x; 1:r4=y; 1:r6=z; 1:r7=a;
}
P0                      | P1                                         ;
r1 = 1                  | r1 = 1                                     ;
r3 = *(u32 *)(r2 + 0)   | r3 = *(u32 *)(r6 + 0)                      ;
r4 += r3                | r7 = atomic_fetch_add((u64*)(r7 + 0), r7)  ;
r4 -= r3                | *(u32 *)(r2 + 0) = r1                      ;
*(u32 *)(r4 + 0) = r1   |                                            ;
*(u32 *)(r6 + 0) = r1   |                                            ;

locations [x;y;z]
exists (0:r3=1 /\ 1:r3=1)
